{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/images",
      "dest": "server.js"
    },
    {
      "src": "/config",
      "dest": "server.js"
    },
    {
      "src": "/exif/(.*)",
      "dest": "server.js"
    },
    {
      "src": "^/$",
      "dest": "/public/index.html"
    },
    {
      "src": "/about",
      "dest": "/public/about.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
